<Window x:Class="proba_database_wpf.Dialogos.AcciónsDllsWindow.AddNewDllWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:proba_database_wpf.Dialogos.AcciónsDllsWindow"
        mc:Ignorable="d"
        Title="AddNewDllWindow" Height="395" Width="583" Closing="Window_Closing">
    <Grid>
        <Grid.RowDefinitions>
            
        </Grid.RowDefinitions>
        
        <!--Componentes-->
        <StackPanel>
            <Label Content="Dll Name" Width="auto" HorizontalAlignment="Center" FontSize="16" Margin="10"></Label>
            <TextBox x:Name="txtDllName" Width="300" Height="30" Margin="10" HorizontalAlignment="Center" FontSize="14"></TextBox>
            <Label Content="Dll Description" Width="auto" HorizontalAlignment="Center" FontSize="16" Margin="10"></Label>
            <TextBox x:Name="txtDescription"  Width="300" Height="130" Margin="10" HorizontalAlignment="Center"></TextBox>
            <Button x:Name="btnAddDll" Content="Add Dll" HorizontalAlignment="Center" Margin="10" Width="150" Height="35" Click="btnAddDll_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
